{
    "name": "FastPLNFlow Dev",
    "dockerComposeFile": [
      "../docker-compose.yml"
    ],
    "service": "api",
    "workspaceFolder": "/app",
    "extensions": [
      "ms-python.python",
      "ms-azuretools.vscode-docker"
    ],
    "settings": {
      "python.defaultInterpreterPath": "/usr/local/bin/python"
    },
    "forwardPorts": [8000, 5672, 15672],
    "postCreateCommand": "pip install --upgrade pip && pip install -r requirements.txt"
  }
  